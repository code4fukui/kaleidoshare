{
  "name": "kaleidoshare",
  "devDependencies": {
    "@monaco-editor/react": "^4.5.1",
    "@simplewebauthn/browser": "^7.2.0",
    "@types/matter-js": "^0.18.3",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "ajv": "^8.12.0",
    "matter-js": "^0.19.0",
    "npm-run-all": "^4.1.5",
    "poly-decomp": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-json-schema-generator": "^1.2.0",
    "tsx": "^3.12.7",
    "vite": "^4.3.5"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:frontend": "vite",
    "dev:backend": "deno run -A --unstable ./server.ts",
    "build": "run-s build:*",
    "build:schema": "ts-json-schema-generator --path 'schema/**/*.mts' --out schema/schema.json --additional-properties false",
    "build:frontend": "vite build",
    "preview": "run-p preview:*",
    "preview:frontend": "vite preview",
    "preview:backend": "deno run -A --unstable ./server.ts",
    "stub": "cd dist && deno run -A https://deno.land/std@0.157.0/http/file_server.ts",
    "test": "node --loader tsx --test ./test/*.test.mts"
  }
}
